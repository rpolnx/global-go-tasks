{
  "name": "global-go-tasks",
  "displayName": "global-go-tasks",
  "publisher": "rpolnx",
  "repository": {
    "type": "git",
    "url": "git@github.com:rpolnx/global-go-tasks.git"
  },
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "globalGoTasks.genGoTestifyTestsFile",
        "title": "Go: Generate Testify Unit Tests For File"
      },
      {
        "command": "globalGoTasks.genGoTestifyTestsFunc",
        "title": "Go: Generate Testify Unit Tests For Function"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}